{
	"extends": "@cloudflare/workers-tsconfig/tsconfig.json",
	"compilerOptions": {
		"module": "CommonJS",
		"types": ["node", "vitest/globals"],
		"jsx": "react"
	},
	"include": ["**/*.ts", "**/*.tsx", "**/*.js"],
	"exclude": [
		"emitted-types",
		"node_modules",
		"*-dist",
		"src/miniflare-cli",
		"templates",
		"**/e2e",
		"src/pages/functions"
	]
}
