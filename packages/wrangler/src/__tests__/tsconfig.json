{
	"extends": "@cloudflare/workers-tsconfig/tsconfig.json",
	"compilerOptions": {
		"module": "esnext",
		"target": "esnext",
		"types": ["node", "vitest/globals"],
		"jsx": "preserve"
	},
	"include": [
		"./**/*.ts",
		"./**/*.tsx",
		"./**/*.js",
		"**/*.d.ts",
		"./**/*.test.[jt]sx?"
	],
	"exclude": ["../selfsigned.d.ts"]
}
