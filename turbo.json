{
	"$schema": "http://turbo.build/schema.json",
	"pipeline": {
		"dev": {
			"persistent": true,
			"cache": false
		},
		"build": { "dependsOn": ["^build"] },
		"test": {
			"dependsOn": ["^build"]
		},
		"test:watch": {
			"cache": false
		},
		"check:lint": {
			"dependsOn": ["^topological"]
		},
		"check:type": {
			"dependsOn": ["^topological"]
		},
		"topological": {
			"dependsOn": ["^topological"]
		},
		"test:ci": {},
		"//#check:format": { "cache": true }
	}
}
